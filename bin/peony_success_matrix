#!/usr/bin/env python
# -*- coding: utf-8 -*-

from peony.hpc import grid_progress, draw_success_matrix
import click

@click.command()
@click.option("-i", "--logfile", help="Path to the logfile")
def main(logfile):
    success = grid_progress(logfile)
    draw_success_matrix(success)

if __name__ == '__main__':
    main()

